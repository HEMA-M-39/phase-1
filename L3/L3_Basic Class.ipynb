    "#task1\n",
    "class Person:\n",
    "    def __init__(self,name):\n",
    "        self.name = name\n",
    "\n",
    "    def greet(self):\n",
    "        return f\"Hi,{self.name}\"\n",
    "        \n",
    "person_instance = Person(\"Alice\")\n",
    "\n",
    "result =  person_instance.greet()\n",
    "\n",
    "print(result)\n",
