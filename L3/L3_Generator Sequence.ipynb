   "source": [
    "#task 5\n",
    "\n",
    "def fib(n):\n",
    "    a, b = 0, 1\n",
    "    for _ in range(n):\n",
    "        yield a\n",
    "        a, b = b, a + b\n",
    "\n",
    "\n",
    "fib_sequence = list(fib(5))\n",
    "\n",
    "\n",
    "print(fib_sequence)  "
   ]
